  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        employee.LastName, customer.Company, COUNT(orders.OrderNo) AS [Число заказов], employee.FirstName
FROM            customer INNER JOIN
                         orders ON customer.CustNo = orders.CustNo INNER JOIN
                         employee ON orders.EmpNo = employee.EmpNo
GROUP BY employee.EmpNo, employee.LastName, customer.Company, employee.FirstName</CommandText>
      </Query>
      <Fields>
        <Field Name="LastName">
          <DataField>LastName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
