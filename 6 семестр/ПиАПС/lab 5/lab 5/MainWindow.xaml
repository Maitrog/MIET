<Window x:Class="lab_5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab_5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="UserText" Grid.Row="0" TextWrapping="Wrap"
                 AcceptsReturn="True" AcceptsTab="True"
                 VerticalScrollBarVisibility="Visible"></TextBox>
        <Button x:Name="Interpret" Grid.Row="1" Content="Interpret"
                Margin="5" Click="Interpret_Click"></Button>
    </Grid>
</Window>
